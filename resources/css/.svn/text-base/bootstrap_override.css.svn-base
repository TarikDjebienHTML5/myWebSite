[class*="span"] {
	background-color: #f5f5f5;
	padding: 1px;
	margin-top: 2px;
	margin-bottom: 2px;
	border-radius: 5px 5px 5px 5px;
	box-shadow: 2px 6px 6px rgba(0, 0, 0, 0.1);
}
.carousel-inner {
    border-radius: 5px 5px 5px 5px;
    border: 5px solid white;
    box-shadow: 2px 2px 2px rgba(0, 0, 0, 0.3);
}
.carousel-caption {
	color: white;
}
